;;; schematra.egg -*- scheme -*-

((synopsis
  "Schematra is a minimalistic web server built on top of spiffy")
 (author "Rolando Abarca")
 (license "BSD")
 (category web)
 (dependencies spiffy
	       base64
	       nrepl
               srfi-1
               srfi-13
	       srfi-18
               srfi-69
	       chiccup
	       medea)
 (component-options
  (csc-options "-O3" "-d0"))
 (components (extension schematra)
	     (extension schematra.body-parser)
	     (extension schematra.test)))
